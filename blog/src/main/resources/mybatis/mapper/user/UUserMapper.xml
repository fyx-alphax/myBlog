<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.program.blog.repository.user.UUserRepository">
    <select id="selectUserInfoById"  resultType="com.program.blog.entity.User">
        select nickname, avatar from user where id=#{id};
    </select>
</mapper>